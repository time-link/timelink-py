      source=historical-source;
      guaranteed=id;
      also=tipo,loc,localizacao,ref,data,ano,obs,substitui;
      arbitrary=acto,amz,nom,carta,pauta,hab,proc,beneficio,chanc,encarte,rol,escritura,
            adenda,arrolamento,cas,bap,b,termo,obito,o,ar,lista,rmerce,merce,let,lbach,capela,
          gov,familia,po,fim,devassa,cartaperdao,denuncia,matricula,ordenancas,milicias,
	  memoria58,apontamentosd,eleicao,juramento,lc,crisma,docregio,docpontificio,docepiscopal,automed,
	  pas,lcc,integr,movacao,geodesc

